<mvc:View xmlns:mvc="sap.ui.core.mvc" controllerName="com.sap.build.standard.zmmtransport.controller.Dialog1" xmlns:m="sap.m">
	<m:Dialog horizontalScrolling="true" showHeader="true" state="None" stretch="false" title="Filter" type="Standard" verticalScrolling="true">
		<m:content>
			<m:Table width="259.00000000px" noDataText="No data" mode="None" showSeparators="All" growing="true" growingThreshold="20"
				growingScrollToLoad="true" items="{/FIELD_FILTERSet}">
				<m:infoToolbar>
					<m:Toolbar width="100%" height="auto" design="Auto" visible="false" enabled="true">
						<m:content>
							<m:Label text="Label" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit"/>
						</m:content>
					</m:Toolbar>
				</m:infoToolbar>
				<m:headerToolbar/>
				<m:columns>
					<m:Column width="auto" hAlign="Left" vAlign="Top" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
						<m:header/>
						<m:footer/>
					</m:Column>
				</m:columns>
				<m:items>
					<m:ColumnListItem type="Active">
						<m:cells>
							<m:ObjectIdentifier title="" text="{F_NAME}" titleActive="true" visible="true"/>
						</m:cells>
					</m:ColumnListItem>
				</m:items>
			</m:Table>
		</m:content>
		<m:beginButton/>
		<m:buttons>
			<m:Button text="Cancel" type="Default" icon="sap-icon://decline" iconFirst="true" width="auto" enabled="true" visible="true"
				iconDensityAware="false" press="_onButtonPress3"/>
			<m:Button text="OK" type="Default" icon="sap-icon://sys-enter" iconFirst="true" width="auto" enabled="true" visible="true"
				iconDensityAware="false" press="_onButtonPress4"/>
		</m:buttons>
		<m:customHeader/>
		<m:endButton/>
		<m:subHeader/>
	</m:Dialog>
</mvc:View>